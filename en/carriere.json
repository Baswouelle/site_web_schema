      "hasPart": {
        "@type": "FAQPage",
        "@id": "https://www.physioactif.com/en/carriere/#faq",
        "name": "Questions About Careers at Physioactif",
        "mainEntity": [
          {
            "@type": "Question",
            "name": "What are the professional development paths at Physioactif?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "We offer different development paths: Mentorship and teaching to share your knowledge, Specialization to develop unique expertise, Leadership to evolve into management/partnership roles, and Flexibility to balance work and personal life."
            }
          },
          {
            "@type": "Question",
            "name": "What makes Physioactif different as an employer?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Paid 96-hour mentorship program and progressive integration with 24 hours of clinical observation, dedicated time slots for patient relationships and your personal projects, solid administrative team that handles non-clinical aspects of your practice."
            }
          },
          {
            "@type": "Question",
            "name": "At which clinics can I work?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "We have 5 clinics in Greater Montreal: Ahuntsic (Montreal), Blainville, Chomedey (Laval), Saint-Eustache and Vaudreuil-Dorion. All positions are available at all clinics."
            }
          },
          {
            "@type": "Question",
            "name": "What types of positions are available?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "We're recruiting for: Physiotherapist ($80,000 - $90,000/year), Physical Rehabilitation Technician ($30/hr), Care Coordinator ($19-26/hr), and Massage Therapist (revenue sharing)."
            }
          }
        ]
